@article{zhang_understanding_2018,
	title = {Understanding and {Mitigating} the {Security} {Risks} of {Voice}-{Controlled} {Third}-{Party} {Skills} on {Amazon} {Alexa} and {Google} {Home}},
	volume = {1},
	url = {https://www.researchgate.net/publication/324982248_Understanding_and_Mitigating_the_Security_Risks_of_Voice-Controlled_Third-Party_Skills_on_Amazon_Alexa_and_Google_Home},
	doi = {arXiv:1805.01525v1},
	abstract = {Assistentes pessoais virtuais (VPA) (por exemplo, Amazon Alexa
e o Google Assistente) hoje dependem principalmente da voz
canal para se comunicar com seus usuários, que no entanto
é conhecido por ser vulnerável, sem autenticação adequada.
O rápido crescimento dos mercados de habilidades VPA abre um novo mercado
avenida de ataque, potencialmente permitindo que um adversário remoto
publicar habilidades de ataque para atacar um grande número de VPA
usuários por meio de dispositivos IoT populares, como Amazon Echo
e o Google Home. Neste artigo, relatamos um estudo que
conclui que tais ataques remotos em grande escala são de fato
realista. Mais especificamente, implementamos dois novos
ataques: agachamento de voz em que o adversário explora
a forma como uma habilidade é invocada (por exemplo, "capital aberto"), usando
uma habilidade maliciosa com nome pronunciado de forma semelhante (por exemplo,
“Ganho capital”) ou nome parafraseado (por exemplo, “capital ganho um
por favor ”) para sequestrar o comando de voz destinado a um diferente
habilidade, e voz disfarçada em que um malicioso
habilidade personifica o serviço VPA ou uma habilidade legítima
para roubar os dados do usuário ou espionar suas conversas.
Esses ataques visam a forma como os VPAs funcionam ou a falha do usuário
concepções sobre suas funcionalidades, e são encontrados para
representam uma ameaça realista por nossos experimentos (incluindo o usuário
estudos e implantações no mundo real) no Amazon Echo
e o Google Home. O significado de nossas descobertas
já foi reconhecido pela Amazon e Google, e
evidenciado pelas habilidades arriscadas descobertas no Alexa
e o Google comercializa pelos novos sistemas de detecção que
construído. Desenvolvemos ainda mais técnicas para remoção automática
proteção desses ataques, que já capturam o mundo real
habilidades que possam representar tais ameaças.},
	language = {en},
	number = {1},
	journal = {arXiv:1805.01525v1 [cs.CR] 3 May 2018},
	author = {Zhang, Nan and Mi, Xianghang and Feng, Xuan and Wang, XiaoFeng and ian, yuan and Qian, Feng},
	month = may,
	year = {2018},
	pages = {16},
}

@misc{governo_federal_estrategia_2018,
	title = {Estratégia {Brasileira} para a {Transformação} {Digital}: {E}-{Digital}},
	shorttitle = {{EBTD}},
	url = {https://www.gov.br/mcti/pt-br/centrais-de-conteudo/comunicados-mcti/estrategia-digital-brasileira/estrategiadigital.pdf},
	abstract = {Esta  Estratégia  Brasileira  para  a  Transformação  Digital  (E-Digital)  é  resultado  de  uma  iniciativa  do  
Governo  Federal,  coordenada  pelo  Ministério  da  Ciência,  Tecnologia,  Inovações  e  Comunicações,  que  
contou com a ativa participação dos membros do Grupo de Trabalho Interministerial responsável pela sua 
elaboração. Este núcleo formulador interagiu com um conjunto de mais de 30 entidades da Administração 
Pública Federal ao longo de todo o processo.
Este  documento  representa  a  conclusão  de  um  caminho  iniciado  por  uma  determinação  presiden-
cial realizada a partir de uma recomendação do Conselho de Desenvolvimento Econômico e Social (CDES), 
em  sua  46ª  Reunião  Plenária
1
. A Presidência da República determinou que fosse elaborada, até o final de 
2017, uma proposta de estratégia de longo prazo para a economia digital.
O resultado reflete também o amplo engajamento do setor produtivo, da comunidade científica e 
acadêmica  e  da  sociedade  civil,  em  diversas  etapas  do  trabalho.  Houve  expressiva  participação  nos  semi
-
nários  e  
workshops
  durante  o  processo  de  formulação  da  Estratégia,  assim  como  na  Consulta  Pública  ao  
documento-base, que recebeu milhares de acessos e contribuições. Isso ensejou a revisão e o aperfeiçoa
-
mento da E-Digital, que se consolida como política pública a ser implementada.
As  rápidas  transformações  na  economia  e  na  sociedade  proporcionadas  pelo  ambiente  digital  
impõem  novos  desafios  à  atuação  do  governo.  As  ações  estratégicas  definidas  têm  foco  no  papel  do 
governo como habilitador e facilitador dessa transformação digital no setor produtivo do país, na capa
-
citação  da  sociedade  para  essa  nova  realidade,  e  na  atuação  do  Estado  como  prestador  de  serviços  e  
garantidor de direitos.
A E-Digital oferece um amplo diagnóstico dos desafios a serem enfrentados, uma visão de futuro, 
um  conjunto  de  ações  estratégicas  que  nos  aproximam  dessa  visão,  e  indicadores  para  monitorarmos  o  
progresso no atingimento de nossos objetivos. 
É importante destacar que a Estratégia deve ser uma política pública “viva”, constantemente acompa
-
nhada, avaliada e ajustada, pois o ritmo global da transformação digital tende a se acelerar.
No entanto, talvez o aspecto mais importante seja o país encarar a transformação digital como uma 
oportunidade ao País para dar um salto qualitativo. As tecnologias digitais proporcionam as ferramentas 
para uma profunda transformação na atuação do próprio governo, na competitividade e produtividade das 
empresas, assim como na capacitação e inclusão na sociedade, para que todos possam se desenvolver e 
prosperar.},
	language = {pt-BR},
	urldate = {2018-03-24},
	author = {{Governo Federal}},
	month = mar,
	year = {2018},
	annote = {Alexandre Mitsuru Kaihara - Big data and social netbanks: are you ready to replace your bank?
EBTD tema C.1 e C.2.Confiança no Ambiente Digital - Proteção de Direitos e Privacidadee Confiança no Ambiente Digital - Defesa e Segurança no Ambiente Digital - Resumo crítico
 
As páginas 13 a 24 do documento da EBTD abordam de que forma é possível ampliar o acesso da população brasileira à Internet e às tecnologias digitais, com qualidade de serviço e economicidade. O documento ...... São apresenados ....
 
 
 
O advento de novas tecnologias implica mutuamente em uma ampliação de alguns aspectos positivos à sociedade, como inclusão social e inovação, e ao mesmo tempo aspectos negativos, como a exposição indevida de privacidade (Federal Trade Comission, ano...). Assim, torna-se mais crítica a preocupação não só com a confiabilidade e segurança das tecnologias, mas também com a garantia dos direitos pessoais (um tema muito evidente para discussão) estendendo-se até às questões políticas ao se tratar de “Big Data e Social Netbanks”, de acordo com a Federal Trade Comission (ano).
 
Dentre esses tecnologias, a grande variedade dos “Internet of Things” e, portanto, de sensores desenvolvidos, a qualidade destes aumentam ao longo do tempo e a veracidade das informações coletadas são bem maiores também (O’LEARY, 2013). Para tanto, o livro da Estratégia Brasileira de Transformação Digital afirma que essas tecnologias aumentam a vulnerabilidade e as ameaças a defesa cibernetica e, de acordo com New York Times, o aumento de sensores de dispositivos computacionais “Internet of Things” têm impulsionado e facilitado o Big Data, não apenas em acesso, mas na compreensão dos dados analisados, aumentando a precisão de previsões e tendências.
 
A ampliação de obtenção de dados tem sido impulsionada também pelos “Financial services apps” do “Payment industries”. Um estudo recente mostrou que 60\% das Instituições financeiras tem utilizado a análise do Big Data como uma vantagem competitiva vital e 63\% dos bancos em todo o mundo acreditam que uma estratégia efetiva do Big Data vão determinar os lideres do mercado nesse setor futuramente. Assim, os “smart banks” podem obter relações e informações de como o usuário utiliza o seus serviços financeiros online, ATM, “branch banking” e outros e vendê-los para facilitar previsões e tendencias de um determinado setor (GESLEVICH-PACKIN et. al.).
 
Os “social networks” estão aderindo ao marcado de serviços financeiros, assumindo alguns serviços tradicionalmente bancários, mas sem serem classificados como tal, por exemplo o Google Wallet (GESLEVICH-PACKIN et. al.). Então, o gigante Facebook obtêm mais e mais informações de seus usuários. Chega a dimensões exorbitantes, por exemplo, a empresa já está autorizada a facilitar determinadas formas de transferência de dinheiro no Estados Unidos, somando em um montante de 2,1 bilhões de dólares ao total em 2013 (GESLEVICH-PACKIN et. al.).
 
Devido ao potencial de abrangência dessas tecnologias, aumenta a preocupação com a confiabilidade, direitos e privacidade sobre a coleta de dados, e, por mais que as cestejondições de uso dos dados dos usuários estejam declaradas no termo de consentimento ou licença de uso, é difícil regular o uso indevido de dados por empresas às quais esses termos e licenças estão associados, enquanto esses empresas não estiverem sob jurisdição da legislação nacional (VIEIRA, 2013). Ainda mais, como regular o acesso aos dados que o provedor está utilizando e que não estão diretamente associados com o termo de consentimento? Portanto, torna-se difícil assegurar uma proteção aos usuários de seus direitos e privacidade no processo de coleta de dados, uma vez que estas ocorrem sem o entendimento por parte das empresas das questões relacionadas à segurança de dados e privacidade (VIEIRA, 2013).
 
No âmbito legislativo, existem códigos que e direitos assegurados pela Constituição Federal, como o Marco Civil da Internet. De acordo com o livro Estratégia Digital, o Marco Civil da Internet tem aplicabilidade plena no espaço cibernético, dependendo da atuação proativa do poder público e de agentes privados da internet. No entanto, Américo Vieira afirma que este não trouxe nenhum acréscimo substancial aos brasileiros com relação aos direitos, uma vez que não explicitou sanções civis, penais mais severas que as já existentes. Também afirma a existência de uma “indústria do dano moral”, que deveria ser tema de discussão para regulamentação jurídica, pois há sucessivas violações no ordenamento jurídico de sociedades empresárias e organizações, irrompendo com a moral objetiva e subjetiva dos brasileiros.
 
O grande problema resultante de uma análise indevida dos dados, que podem ser oriundos de invasões não consentidas sobre o direito à privacidade usuário é que podem trazer consequências negativas para o mesmo. As principais, citadas em “Big Data: tool for inclusion or exclusion? Understanding the issues (FTC report)”, são: Mais pessoas tendo oportunidades negadas por engano da análise de seus dados privados com base em outros padrões; criação e reforço de disparidades e preconceitos já existentes; exposição de informações confidenciais; Ajudam a segmentar o mercado consumidor frágil por fraude; Novas justificativas para a exclusão; Bens e serviços com preços mais elevados para comunidades de baixa renda e enfraquecimento da eficácia de escolha do consumidor.
 
Em uma pesquisa realizada por Danice Kinner Borba et. al., verificou-se que dentro da população de análise, mais de 80\% das pessoas têm consciência do Big Data em navegações da internet e 45\% acredita que é uma ferramenta útil que facilita a navegação, no entanto muitas ainda possuem receio do que é feito com esses dados, como já explorado por Craig e Ludloff, 2011 e 84\% sentiriam incomodados por um direcionamento ofensivo ou comprometedor.
 
Assim, Big Data precisa haver uma maior regulação em prol de uma maior segurança e garantia de direitos. Algumas propostas de ações estratégicas são citadas pelo livro Estratégia Digital, sendo as mais relevantes: Promover uma política nacional de segurança cibernética, incluindo a criação de uma instância nacional deliberação de um sistema nacional de segurança cibernética, abrangendo os setores privados e públicos; Promover o marco legal de segurança cibernética, articulando com o direito penal e processual já existente e avançar na previsão de novos instrumentos de investigação para o mundo digital; Elaborar mecanismos que visem a cooperação de entes governamentais, federados, do setor privado para adoção de melhores práticas, compartilhamento de informações, adoção de padrões mais adequados de segurança; Realizar companhas de conscientização sobre o tema da segurança da informação e subsidiar pesquisas no setor de defesa e segurança cibernética, visando promover a autonomia tecnológica; Reforçar a cooperação internacional entre autoridades e entre provedores de acesso e conteúdo em diferentes países, para garantir a aplicação da lei no ambiente digital.
 
 },
	annote = {Ana Clara Rodrigues de Abreu – EBTD tema D. Educação e Capacitação Profissional – Resumo crítico.
As páginas 45 a 52 do documento da EBTD abordam os benefícios que o uso de tecnologias digitais na educação traz não só para os alunos como para os profissionais envolvidos no ensino, também frisam a importância do conheimento tecnológico para o mercado de trbalho. O documento apresenta também alguns empecilhos para a implementação das TICs na educação como falta de infraestrutura, especialmente nas escolas rurais, e também a falta de profissionais qualificados.
O texto traz diversos dados relevantes que justificam a necessidade dessa reformulação da educação, além de detalhar os cuidados que uma política pública com relação os uso das TIC na educação precisa ter para ser eficaz e de explicitar quais as medidas a serem tomadas para atingir os objetivos previstos.
Entendo que o tema é de extrema urgência visto a situação precária do sistema educacional atual. Porém, sua aplicação deve ser muito cuidadosa, pois, como dito no documento da EBTD, do contrário poderia ter efeitos adversos que fogem aos objetivos iniciais.
 
Meu artigo base sobre cyber educação está intimamente ligado a este tema, um problema abordado em ambos e que eu creio ser a maior dificuldade do Brasil é a falta de infraestrutura.  Analisando as questões apresentadas por ambos, concluo que se o plano de implementação das tecnologias educacionais no ensino regular e na capacitação profissional for executado como o previsto na EBTD o Brasil avançará significativamente em direção à eficácia do modelo de cyber educação proposto em meu artigo de forma ética.},
	annote = {Andrey Calaça Resende - EBTD tema G. Transformação Digital: Cidadania e Governo.

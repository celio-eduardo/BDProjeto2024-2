@article{hammon_alterity_2016,
	title = {Alterity and freedom of information on the internet: the loss of net neutrality in contemporary literature},
	volume = {45},
	issn = {0095-2737},
	url = {https://dl.acm.org/citation.cfm?id=2874253},
	doi = {10.1145/2874239.2874253},
	abstract = {Giving a short overview of the technical innovation of Deep Packet Inspection and "Internet fast-lanes", this paper shows the ethical dimension of giving up the founding principle of the open Internet. These new means of network management are fiercely discussed because of their inherent threat of censorship and attack on privacy. This paper will attempt to explain why the experience of otherness and both the freedom of information and the freedom of expression are endangered if Net Neutrality is no longer protected. Furthermore, it shows by means of some representative examples how this subject is reflected in contemporary literature.},
	number = {3},
	journal = {ACM SIGCAS Computers and Society - Special Issue on Ethicomp},
	author = {Hammon, Jasmin},
	month = jan,
	year = {2016},
	pages = {91--99},
	annote = {RESUMO
O artigo de Jasmin Hammon “Alteridade e Livre Acesso à Informação na Internet – A perda da Neutralidade de Rede na literatura contemporânea” analisa os motivos pelos quais o paradigma de tecnologia das “Internet fast-lanes” geram dilemas na sociedade. Na primeira seção é feita uma análise da situação política global da Neutralidade de Rede através de notícias publicadas na mídia. Posteriormente, o artigo discute como o paradigma de tecnologia influencia a liberdade de expressão, a experiência de alteridade e, por consequência, o pensamento crítico. Por fim, narrativas da literatura contemporânea são analisadas para encontrar como a neutralidade de rede pode afetar uma sociedade por cerceamento da liberdade de expressão e da informação independente.
O artigo foi publicado em 2015 e, por conta disso, o cenário político traçado para a Neutralidade de Rede nos EUA, Europa e Canadá passaram por mudanças que devem ser levadas em consideração. À época, a Comissão Federal de Comunicação (FCC), agência reguladora norte-americana equivalente à Anatel no Brasil, havia estabelecido regras de neutralidade de rede que foram derrubadas e vem sendo atacadas pelo novo chairman da FCC do atual governo Republicano, Ajit Pai. 
À época, o Canadá, representado pelo CRTC, havia adotado um entendimento similar ao da FCC, favorável à neutralidade. A primeira ministra da Alemanha, Angela Merkel, era favorável à criação de “Internet fast-lanes” para que as detentoras de infraestrutura pudessem participar dos ganhos com serviços de valor agregado e acumular capitais para fazer os investimentos necessários para a renovação da infraestrutura de telecomunicações do país. A França também tinha posição favorável às operadoras.
Na apresentação de detalhes técnicos da Neutralidade de Rede, a autora afirma que a capacidade das redes estava quase exaurida e que a largura de banda da banda larga móvel era limitada. O aumento de demanda por dados criava um desequilíbrio que criava um risco de colapso em toda a rede, a não ser que a infraestrutura fosse expandida ou que as redes fossem geridas de um modo mais eficiente, ou seja, através de “Internet fast-lanes”. Esse recurso, um paradigma de tecnologia que muda a maneira como o tráfego de dados é transmitido através da rede, prioriza a transmissão de dados de certos serviços em detrimento de outros.
A princípio, a ideia de que a largura de banda da banda larga móvel é fisicamente limitada não leva em consideração os avanços tecnológicos do 5G, em que a velocidade de transmissão sem fio aumentará consideravelmente, e das conexões por meio de fibra ótica e a popularização do FTTH (Fiber to the home). Essa ideia descarta uma solução para o problema de investimento em P\&D para novas tecnologias de rede e estímulo para investimentos em infraestrutura. 
Para se fazer o gerenciamento do tráfego de dados, algum modelo precisa ser utilizado. É na escolha do modelo que estão as ameaças à equidade de oportunidades de mercado e à liberdade de expressão e acesso à informação. Caso a priorização de dados seja feita por meio financeiro, as empresas que detêm maior poder de capital poderão monopolizar o mercado. Caso esse assunto fique a cargo de regulamentação governamental, há riscos do governo censurar informações por razões políticas. A tecnologia que possibilitaria essas falhas de uso da tecnologia é o Deep Packet Inspection (DPI).
         Em um cenário de Neutralidade de Rede, apenas os cabeçalhos dos pacotes de dados são lidos pelos ISPs. A tecnologia DPI possibilita a leitura de camadas que contém a informação transmitida pelo pacote de dados, o que coloca em risco a privacidade dos dados transmitidos e pode tornar não transparente o real modelo de priorização (ou não priorização) na transmissão por parte dos ISPs. Alguns exemplos de bloqueios de dados e serviços VoIP de concorrentes por parte dos ISPs foram dados no texto.
         Assim, é contextualizado o dilema da introdução de “internet fast-lanes” e DPI. De um lado, os ISPs alegam a necessidade da tecnologia para o aumento da eficiência técnica da rede e da competitividade do mercado. Por outro lado, os usuários alegam que as tecnologias ameaçam a liberdade de expressão.},
}

@misc{leopold_alienation_2018,
	title = {Alienation},
	url = {https://plato.stanford.edu/entries/alienation/},
	abstract = {The concept of alienation identifies a distinct kind of psychological or social ill; namely, one involving a problematic separation between a self and other that properly belong together. So understood, it appears to play a largely diagnostic role, perhaps showing that something is awry with liberal societies and liberal political philosophy. Theories of alienation typically pick out a subset of these problematic separations as being of particular importance, and then offer explanatory accounts of the extent of, and prognosis for, alienation, so understood. Discussions of alienation are especially, but not uniquely, associated with Hegelian and Marxist intellectual traditions.

The present entry clarifies the basic idea of alienation. It distinguishes alienation from some adjacent concepts; in particular, from ‘fetishism’ and ‘objectification’. And it elucidates some conceptual and normative complexities, including: the distinction between subjective and objective alienation; the need for a criterion by which candidate separations can be identified as problematic; and (some aspects of) the relation between alienation and ethical value. The empirical difficulties often generated by ostensibly philosophical accounts of alienation are acknowledged, but not resolved.},
	urldate = {2019-07-04},
	journal = {Stanford Encyclopedia of Philosophy},
	author = {Leopold, David},
	month = aug,
	year = {2018},
}


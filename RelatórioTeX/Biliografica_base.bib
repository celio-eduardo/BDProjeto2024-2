@article{thornton_incorporating_2017,
	title = {Incorporating {Ethical} {Considerations} {Into} {Automated} {Vehicle} {Control}},
	volume = {18},
	issn = {1524-9050},
	doi = {10.1109/TITS.2016.2609339},
	abstract = {Not only do automated vehicles need to meet specifications for technical performance, they also need to satisfy the societal expectations for behavior in traffic with humans. Societal expectations, such as accident avoidance and adherence to traffic laws, have their foundation in core moral issues found in philosophy and ethics. Thus, engineers designing control algorithms for automated vehicles can benefit from applying principles and frameworks from philosophy to drive design decisions. In particular, we use a set of ethical frameworks to map design decisions for a model predictive control problem to philosophical principles. Deontology, a rule-based ethical framework, motivates the development of constraints on the system. Consequentialism, a cost-based ethical framework, motivates the construction of the objective function. The choice of weights is guided by the concepts of virtue ethics and role morality to determine behavior for different types of vehicles. The strong link between ethical principles and actual vehicle behavior developed through this approach is demonstrated experimentally by implementing alternative design choices on a test vehicle in a simple driving scenario.},
	number = {6},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Thornton, S. M. and Pan, S. and Erlien, S. M. and Gerdes, J. C.},
	month = jun,
	year = {2017},
	keywords = {Ethics, Autonomous vehicles, ethics, actual vehicle behavior, automated vehicle control, Autonomous automobiles, control algorithm design, control system synthesis, cost-based ethical framework, ethical aspects, ethical principles, Law, model predictive control, model predictive control problem, Optimization, philosophical principles, predictive control, road traffic control, road vehicles, Roads, rule-based ethical framework, vehicle control, Vehicles},
	pages = {1429--1439},
}


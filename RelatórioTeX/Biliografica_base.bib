@article{beltagui_overcoming_2019,
	title = {Overcoming the monetization challenge in freemium online games},
	volume = {119},
	issn = {0263-5577},
	url = {https://doi.org/10.1108/IMDS-08-2018-0350},
	doi = {10.1108/IMDS-08-2018-0350},
	abstract = {Online games based on a freemium business model face the monetization challenge. The purpose of this research is to examine how players’ achievement orientation, social orientation and sense of community contribute to willingness to pay (WtP). A multi-method study of an online game community is used. Interviews and participant observation are used to develop an understanding of social and achievement orientations followed by the development of hypotheses that are tested using survey data. The findings indicate that a sense of community is positively related with WtP, whereas satisfaction or dissatisfaction with the service provider is not. We examine the moderating role of players’ achievement orientation and social orientation and find that while a stronger connection to the online game community may encourage achievement-oriented players to pay, the opposite is indicated for socially-oriented players. Decision makers need to understand that not all players are potential payers; while socially-oriented users can help to maintain and grow the community, achievement-oriented players are more likely to pay for the value they extract from the community. While communities are held together by people with common interests, which intuitively suggests that WtP increases with the strength of connection to the community, we find this only applies in the case of players with an achievement orientation. For those with a social orientation, WtP may actually decrease as their connection to the online game community increases. These perhaps counter-intuitive findings constitute a novel contribution of value for both theory and practice.},
	language = {English},
	number = {6},
	urldate = {2022-02-04},
	journal = {Industrial Management \& Data Systems},
	author = {Beltagui, Ahmad and Schmidt, Thomas and Candi, Marina and Roberts, Deborah Lynn},
	month = jan,
	year = {2019},
	keywords = {Achievement orientation, Mixed methods, Monetization, Online games, Social orientation},
	pages = {1339--1356},
}

@article{krumholz_big_2014,
	title = {Big {Data} {And} {New} {Knowledge} {In} {Medicine}: {The} {Thinking}, {Training}, {And} {Tools} {Needed} {For} {A} {Learning} {Health} {System}},
	volume = {33},
	issn = {0278-2715},
	url = {https://www.healthaffairs.org/doi/10.1377/hlthaff.2014.0053},
	doi = {10.1377/hlthaff.2014.0053},
	abstract = {Big data na medicina – grandes quantidades de dados de saúde acumulados de pacientes e populações e análises avançadas que podem dar significado a esses dados – têm a perspectiva de se tornar um motor para a geração de conhecimento necessária para atender às extensas necessidades de informações não atendidas dos pacientes , médicos, administradores, pesquisadores e formuladores de políticas de saúde. Este artigo explora as maneiras pelas quais o big data pode ser aproveitado para avançar na pesquisa de previsão, desempenho, descoberta e eficácia comparativa para lidar com a complexidade de pacientes, populações e organizações. A incorporação de big data e análises de última geração em pesquisas e práticas de saúde clínica e populacional exigirá não apenas novas fontes de dados, mas também novas ideias, treinamento e ferramentas. Adequadamente utilizados, esses reservatórios de dados podem ser uma fonte praticamente inesgotável de conhecimento para alimentar um sistema de saúde em aprendizagem.},
	language = {Inglês},
	number = {7},
	journal = {HighWire Press (Free Journals)},
	author = {Krumholz, Harlan M.},
	month = jul,
	year = {2014},
	pages = {1163--1170},
}


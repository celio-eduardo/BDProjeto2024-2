@article{vechiato_recomendacoes_2013,
	title = {Recomendações de usabilidade e de acessibilidade em projetos de ambientes informacionais digitais para idosos},
	url = {http://repositorios.questoesemrede.uff.br/repositorios/handle/123456789/2044},
	abstract = {Usability and accessibility when applied in digital information environments, provide ease of use and information access, respectively. Therefore, we aimed to discuss these concepts and apply them in digital environments specific to seniors through bibliographic and documental of specific recommendations in projects for elderly users. This study was conducted in conjunction with the students of the Open University of the Third Age (UNATI)- UNESP - Marilia, who assisted in the classification of recommendations for usability and  accessibility in essential (Priority 1), important (Priority 2) and optional (Priority 3). The results show the importance of this type of study for the digital and social inclusion of older people by facilitating elements of access and use of information that provide the construction of informational architectures more inclusive. The resulting recommendations can be used as a starting point for the design of specific digital environments for seniors in a heuristic evaluation, for example. However, it remains important to apply usability tests with users, inclusive to validate the application of these recommendations, since different contexts may arise from the application of different elements, resources, and information services.},
	language = {pt\_BR},
	urldate = {2020-08-23},
	author = {Vechiato, Fernando Luiz and Vidotti, Silvana Aparecida Borsetti Gregorio},
	month = oct,
	year = {2013},
}

@article{anjos_recomendacoes_2015,
	title = {Recomendações de usabilidade e acessibilidade para interface de telefone celular visando o público idoso},
	volume = {25},
	issn = {1980-5411, 0103-6513},
	url = {http://www.scielo.br/scielo.php?script=sci_arttext&pid=S0103-65132015000400791&lng=pt&tlng=pt},
	doi = {10.1590/0103-6513.091312},
	abstract = {Resumo Este trabalho estuda a relação do idoso com o telefone celular e estabelece algumas recomendações que podem auxiliar a melhorar essa interação. Investiga o uso do telefone celular, que é o equipamento eletrônico que mais cresce em vendas no mundo, pelo público idoso, a fim de identificar suas necessidades, dificuldades e compreensão quanto a ícones, nomenclaturas e agrupamento das funções. Para isso foram utilizados questionário de satisfação, arranjo de cartas e teste de compreensão de ícones. Com isso foram identificadas recomendações de usabilidade e acessibilidade para a interface do celular, a fim de auxiliar e orientar desenvolvedores de interface na tomada de decisões durante o processo de design. Concluiu-se que os idosos precisam de funções fáceis de entender, terminologias claras e funções agrupadas de acordo com a sua compreensão, a fim de proporcionar uma interface mais amigável.
          , 
            Abstract This paper studies the relationship between the elderly and the mobile phone, and the research provides several recommendations that can help improve this interaction. Because mobile phones are the fastest-growing segment of electronic equipment in sales worldwide, this work investigates the use of mobile phones by the elderly to identify their needs, difficulties and understanding of the icons, classifications and grouping functions. A satisfaction questionnaire, card sorting and comprehension test icons were used to determine the results. Usability and accessibility recommendations were identified for the mobile interface to assist and guide interface developers during the design process. It was concluded that the elderly need easier functions, clearer terminology without double meaning, a friendlier interface and functions that are grouped according to their understanding.},
	language = {pt},
	number = {4},
	urldate = {2019-05-12},
	journal = {Prod.},
	author = {Anjos, Thaiana Pereira dos and Gontijo, Leila Amaral},
	month = nov,
	year = {2015},
	pages = {791--811},
}

@article{stahl_ethics_2016,
	title = {Ethics of healthcare robotics: {Towards} responsible research and innovation},
	volume = {86},
	url = {https://ac.els-cdn.com/S0921889016305292/1-s2.0-S0921889016305292-main.pdf?_tid=6693c031-51ef-48d5-98b9-57445c3a402c&acdnat=1530706805_8414bf2128d610a0900426d7b99877c6},
	doi = {101016},
	abstract = {How can we best identify, understand, and deal with ethical and societal issues raised by healthcare
robotics? This paper argues that next to ethical analysis, classic technology assessment, and philosophical
speculation we need forms of reflection, dialogue, and experiment that come, quite literally, much closer
to innovation practices and contexts of use. The authors discuss a number of ways how to achieve that.
Informed by their experience with ‘‘embedded’’ ethics in technical projects and with various tools and
methods of responsible research and innovation, the paper identifies ‘‘internal’’ and ‘‘external’’ forms
of dialogical research and innovation, reflections on the possibilities and limitations of these forms of
ethical–technological innovation, and explores a number of ways how they can be supported by policy at
national and supranational level.},
	journal = {Robotics and Autonomous Systems},
	author = {Stahl, Bernd Carsten and Coeckelbergh, Mark},
	year = {2016},
	pages = {152--161},
}


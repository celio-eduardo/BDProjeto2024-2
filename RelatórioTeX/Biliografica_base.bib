@article{santos_mulheres_2021,
	title = {Mulheres e games: {Um} estudo sobre a objetificação das mulheres nos jogos eletrônicos},
	url = {http://repositorio.ufu.br/bitstream/123456789/31196/3/MulheresEGames.pdf},
	abstract = {In view of the expansion of the electronic games market, these, socially conceived as
part of the male universe, start to attract more and more women. This research aims to analyze
the process of appropriation and objectification suffered by women when they find
themselves facing a highly masculinized scenario such as the world of electronic games. The
research does not aim only to analyze the representativeness of women, but mainly to show
how patriarchal power relations, which have a material and ideological basis, are present in
several environments, including cyber spaces. Throughout the text, the forms of
objectification of women both as characters and as players are exposed, as well as the nuances
of the process of appropriation of time, work and women's sexuality.},

@article{ana_luiza_s_milson_indra_matsiendra_c_d_ribeiro_izabela_a_andrade_julia_m_m_goncalves_luisa_m_laboissiere_marcella_d_ferreira_daniel_h_dalip_michele_a_brandao_elas_2020,
	title = {Elas na {Ciência}: {Website} com {Jogos} para {Divulgar} {Personalidades} {Femininas}},

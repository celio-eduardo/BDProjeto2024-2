	author = {Sommadossi, Guilherme},
	month = oct,
	year = {2023},
}

@misc{rodrigues_pirataria_nodate,
	type = {Educação},
	title = {Pirataria e o acesso ao consumo},
	url = {https://mundoeducacao.uol.com.br/sociologia/a-pirataria-crime.htm},
	abstract = {A pirataria de produtos culturais, como videogames, levanta questões legais e sociais complexas. Embora o Código Penal classifique a reprodução não autorizada de obras protegidas como crime, é importante considerar que muitos consumidores recorrem à pirataria devido à falta de recursos para adquirir produtos originais. Isso destaca a necessidade de equilibrar a proteção dos direitos autorais com o acesso ao consumo cultural para pessoas de baixa renda. Algumas empresas buscam soluções alternativas para combater a pirataria, como tornar produtos culturais mais acessíveis e baratos, como visto no caso de serviços de streaming de música. No entanto, persistem desafios, especialmente no combate à pirataria de produtos de luxo, que pode estar ligada a atividades criminosas e causar danos econômicos e à saúde pública. O debate em torno da pirataria deve abranger não apenas os direitos dos criadores, mas também o acesso da população aos bens culturais, e o barateamento do acesso tem se mostrado uma estratégia eficaz na redução desse problema.},
	language = {Portuguese},
	urldate = {2023-09-19},
	journal = {Mundo Educação},
	author = {Rodrigues, Lucas},
}


@article{lin_effects_2020,
	title = {Effects of videogame-based intervention on postural control in youth with autism spectrum disorder},
	url = {http://scholarworks.csun.edu/handle/10211.3/217120},
	abstract = {Interactive videogame systems have gained popularity as a therapeutic modality in clinical settings. Participating in Nintendo Wii activities, such as Wii-Fit exercise games, has been shown to improve postural control among people with disabilities including children with developmental disabilities. However, no study has used Wii activities to improve postural control in individuals with autism spectrum disorder (ASD); a population that often displays underdeveloped postural control. The purpose of this study was to evaluate the effects of Wii activities on postural control during static and dynamic activities in youth with ASD. Seven youth with ASD played Wii games of their choice (Wii-Fit, Just Dance, \& Just Dance for Kids) for 40 minutes twice a week for eight weeks. Six youth with ASD in the control group continued their regular daily routines. A force plate (NeuroComÂ® Balance Manager) was used to assess participantsâ€™ postural control before and after the intervention. Postural control was examined by measuring COG sway velocity and time during double-legged stance, unilateral stance, and step/quick turns. A 2x2 mixed model analysis of variance (ANOVA) showed a significant group x time interaction in a number of postural control assessments (P{\textless} 0.05). When a significant group x time interaction was observed, paired t-tests with Bonferroni correction (P{\textless} 0.01) were used to examine pre- and post-intervention scores in the intervention and control groups. The Wii activity group had a statistically significant increase in time during left unilateral stance with eyes closed (p= 0.003) while the control group did not show a significant increase (p= 0.146). A statistically significant decrease was found in time during the left-turn for the Wii activity group (p= 0.001) showing a greater decrease in time than the control group (p= 0.333). Results of all but one of the remaining assessments (right-turn time) showed greater improvement for the Wii activity group than the control group but did not reach significance. Children who participated in an eight-week Wii activity program improved postural control. Game-based physical activities may be an effective method to improve postural control in youth with ASD.},
	author = {Lin, Jae},
	month = aug,
	year = {2020},
}

@article{konradt_phishing_2016,
	title = {Phishing: {An} economic analysis of cybercrime perpetrators},
	volume = {58},
	issn = {0167-4048},
	url = {http://www.sciencedirect.com/science/article/pii/S0167404815001844},
	doi = {10.1016/j.cose.2015.12.001},
	abstract = {Abstract
Cybercrime, one of the most important security topics, will continue to emerge as a more critical security threat within the next years. Among the different attacks, phishing is of special interest because of its negative impact for the economy. In this paper, we develop a simulation study based on the work of Fultz and Grossklags. To extend their analysis of cybercrime by an economic view, we customized their model and used it as basis for our analysis. Based on the data from recent literature, our assessment gives insights into the perpetrator's behavior and allows us to quantify the effectiveness of countermeasures. Due to the fact that mainly risk-seeking persons are responsible for these attacks, countermeasures aiming at increasing the penalties are not very effective. We discovered that better control of dark markets to prevent the trading of stolen data has a much higher impact. In general, results of our simulation can be used to analyze the perpetrator's economic motives and to establish a basis for effective countermeasures.},
	journal = {Computers \& Security},
	author = {Konradt, Christian and Schilling, Andreas and Werners, Brigitte},
	month = may,
	year = {2016},
	keywords = {Cybercrime, Economic analysis, @Risk, Information security, Phishing, Simulation},
	pages = {39--46},
}

@article{gomes_de_amorim_gerenciamento_2016,
	title = {{GERENCIAMENTO} {DE} {TESTE} {DE} {SOFTWARE}: {UM} {COMPARATIVO} {ENTRE} {FERRAMENTAS} {OPEN} {SOURCE}},
	volume = {14},
	url = {https://doaj.org/article/d43433c9924c4382818186d06655f1c0},
	abstract = {Tendo em vista a atenção dada à qualidade dentro de um projeto de desenvolvimento de sistemas de informação, uma das maiores preocupações atuais é a busca por novas técnicas que auxiliem o processo de execução e planejamento de teste. A utilização de ferramentas de gestão de teste tem agregado confiança e qualidade ao desenvolvimento, mas ainda é um problema encontrar a ferramenta certa para cada projeto. Diante dessa dificuldade o objetivo deste artigo é analisar ferramentas open source para gestão de teste de software, visando facilitar a escolha das mesmas para utilização em projetos de desenvolvimento de sistemas de informação. Visando alcançar este objetivo, executamos uma revisão da literatura, realizamos um comparativo entre as ferramentas analisadas e fizemos uma análise de cada ferramenta comparada a características de qualidade oriundas da NBR ISO/IEC 9126. Como resultado, identificamos qual das ferramentas estudadas dá suporte ao maior número de situações vividas dentro do desenvolvimento de sistemas de informação. Além disso, descobrimos qual ferramenta atende de forma mais satisfatória aos pontos analisados diante da NBR ISO/IEC 9126. Assim esperamos facilitar e dar uma melhor compreensão na escolha de ferramentas de gestão de teste utilizadas em projetos de desenvolvimento de sistemas de informação.},
	journal = {Gestão.org, 01 August 2016, Vol.14(Especial), pp.296-302},
	author = {Gomes de Amorim, Deivison and Farias Júnior, Ivaldir and Leitão Junior, Nelson and Mendonça Teixeira, Marcelo},
	month = aug,
	year = {2016},
	pages = {7},
}


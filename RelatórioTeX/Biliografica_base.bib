@article{maia_o_nodate,
	title = {O {CONCEITO} {DE} {MEIO} {TÉCNICO}-{CIENTÍFICO}-{INFORMACIONAL} {EM} {MILTON} {SANTOS} {E} {A} {NÃO}-{VISÃO} {DA} {LUTA} {DE} {CLASSES}},
	url = {http://www.seer.ufu.br/index.php/caminhosdegeografia/article/view/16629/9244},
	abstract = {Este texto visa discutir o conceito de meio técnico-científico-informacional em Milton
Santos. A tese que defende-se aqui sustenta-se na idéia de que no processo de
construção do conceito, Milton Santos considerou indiretamente ou de modo secundário
a luta de classes. Isto tem implicações metodológicas e políticas de certa importância.
Após discutir como o autor elaborou o conceito em pelo menos duas décadas
considerando a questão, apresentamos em seguida nossa apreciação crítica. Para a
realização deste estudo, analisamos fundamentalmente a obra A Natureza do Espaço,
texto no qual ele desenvolve de modo mais acabado o conceito de meio técnicocientífico-informacional.
Consideramos também, de modo mais secundário, um conjunto
de outros textos nos quais ele aborda direta ou indiretamente a questão. Nossa intenção
não foi desconsiderar a contribuição deste autor para as pesquisas em Geografia. Pelo
contrário, pretende-se, a partir do que ele nos legou, aprofundar as discussões relativas
a este conceito, que muito tem ainda de elementos explicativos da realidade.},
	author = {Maia, Lucas},
	keywords = {luta de classes, meio técnico-científico-informacional, Milton Santos},
}


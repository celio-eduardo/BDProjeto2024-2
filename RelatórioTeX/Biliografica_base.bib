@article{carvalho_veiculos_nodate,
	title = {{VEÍCULOS} {AUTÔNOMOS} {NO} {BRASIL}: {SITUAÇÕES} {DILEMÁTICAS} {ENVOLVENDO} {PROGRAMADORES} {E} {ESTADO} {DE} {NECESSIDADE}},
	url = {https://www.ufsm.br/app/uploads/sites/563/2019/09/5.12.pdf},
	abstract = {Os veículos autônomos, que se autoconduzem, sem qualquer interferência de um condutor humano,

@article{mark_coeckelberghwessel_reijers_cryptocurrencies_nodate,
	title = {Cryptocurrencies as {Narrative} {Technologies}},
	url = {https://www.scinapse.io/papers/2239846528},
	abstract = {Transitions in monetary technologies raise novel ethical and 
philosophical questions. One prominent transition concerns the 
introduction of cryptocurrencies, which are digital currencies 
based on blockchain technology. Bitcoin is an example of a 
cryptocurrency. In this paper we discuss ethical issues raised by 
cryptocurrencies by conceptualising them as what we call 
“narrative technologies”. Drawing on the work of Ricoeur and 
responding to the work of Searle, we elaborate on the social and 
linguistic dimension of money and cryptocurrencies, and explore 
the implications of our proposed theoretical framework for the
ethics of cryptocurrencies. In particular, taking a social-narrative 
turn, we argue that technologies have a temporal and narrative
character: that they are made sense of by means of individual and 
collective narratives but also themselves co-constitute those 
narratives and inter-human and social relations; configuring 
events in a meaningful temporal whole. We show how 
cryptocurrencies such as Bitcoin dynamically re-configure social 
relations and explore the consequent ethical implications.},
	journal = {Cryptocurrencies as Narrative Technologies},
	author = {Mark CoeckelberghWessel Reijers},
}

@article{pedro_marinelli_de_carvalho_as_nodate,
	title = {{AS} {TECNOLOGIAS} {DA} {INFORMAÇÃO} {E} {COMUNICAÇÃO} {NO} {ENSINO} {DE} {LÍNGUAS} {ESTRANGEIRAS}: {ESTADO} {DA} {ARTE}},
	url = {https://periodicos.fclar.unesp.br/entrelinguas/article/view/17032/15639},
	abstract = {Em  decorrência  da  pandemia,  do  ensino  remoto  emergencial  e  da  crescente  presença das Tecnologias Digitais da Informação e da Comunicação (doravante TDICs) na vida em  sociedade  e  no  ensino  de  Línguas  Estrangeiras  (doravante  LE),  nota-se  um  aumento  do  número de estudos investigando benefícios e desafios dos recursos digitais na Educação. Nesse contexto, este trabalho visa traçar o estado da arte sobre o uso de TDICs no ensino de LE, além de   abordar   tópicos   como   letramento   digital   e   competência   midiática,   profundamente entrelaçados  à  temática.  Após  a  análise  dos  dados  coletados,  observamos  que  é  crescente  o  número de estudos sobre TDICs com a inserção de tais recursos no campo da Educação, que trabalhos na área de inglês como LE predominam, que o número de publicações é maior em periódicos e anais de eventos e, por fim, que os estudos, em geral, tendem a focar na tecnologia como ferramenta de aprendizagem.},
	author = {Pedro Marinelli de CARVALHO},
}

@article{noauthor_cyber_nodate,
	title = {Cyber education: towards a pedagogical and heuristic learning},
	url = {https://dl.acm.org/doi/abs/10.1145/2874239.2874266},
	abstract = {Os constantes e rápidos investimentos em Tecnologias da Informação e Comunicação (TICs) permitiram o crescimento na qualidade das respostas de informação disponíveis na internet, o que requer considerar e abordar os tipos de acesso físico, financeiro, sociodemográfico, cognitivo, de design, institucional, político e cultural. O principal objetivo deste artigo é revisar as TICs atuais e emergentes e o uso de suas ferramentas de aplicação na Educação, que é dominada pelo paradigma linear na interação e na informação, uma vez que a interatividade não está sendo aceita como um princípio orientador. O conceito de e-learning se baseia na ideia de que a pedagogia sustentada tecnologicamente inclui conhecimento suficiente em relação aos desejos dos processos de aprendizado, que são um processo mental incorporado em uma cultura e também desafios, não apenas conceitos. A aprendizagem é um processo que os seres humanos vêm tentando dominar por muitos séculos. No entanto, existem tantas maneiras diferentes de realizar o processo que às vezes é muito difícil determinar qual é a melhor para uma determinada situação. Um tipo de aprendizado é a aprendizagem heurística. Por meio desse método, os alunos devem descobrir coisas por si mesmos, por meio da resolução de problemas, raciocínio indutivo ou simplesmente por tentativa e erro. Descobrir coisas por si mesmo, aprender com a experiência em vez de livros. Em muitas situações, parece que a aprendizagem heurística é a mais adequada quando alguém realmente acredita em algo quando o experimenta pessoalmente. Isso é realmente o que a aprendizagem heurística se trata. Esse modelo de aprendizado, em formato online, é adaptado ao aprendiz adulto que normalmente possui um senso de autodireção relacionado a interesses individuais, metas, pontos fortes e experiência prévia. Além disso, a teoria pedagógica do conectivismo nasceu como resposta ao rápido desenvolvimento das TIC, que influencia fortemente a educação e cuja abordagem para a resolução de problemas se baseia no uso de animações de simulação, fazendo com que os alunos alterem parâmetros e verifiquem ou busquem a resolução do problema, aplicando sua necessidade de busca intuitiva pelo método heurístico; a informação é transmitida por meio de imagens, sem a aplicação de textos longos. Após uma série de experimentos de simulação, os alunos verificam seus resultados com um cálculo e um experimento real.},
	journal = {Cyber education: towards a pedagogical and heuristic learning},
}


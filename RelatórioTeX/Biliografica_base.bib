@article{lopez_internet_2019,
	title = {Internet, {Big} data y nuevas tecnologías: repercusiones y respuestas del ordenamiento jurídico {\textbar} {Internet}, {Big} data and new tecnologies: implications and responses from the legal system},
	copyright = {Copyright (c) 2019 Cuadernos Electrónicos de Filosofía del Derecho},
